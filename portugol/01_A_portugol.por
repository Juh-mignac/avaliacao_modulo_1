programa {
  funcao inicio() {

    inteiro numero1, numero2

    escreva ("Digite número 1\n")
    leia (numero1)

    escreva ("Digite número 2\n")
    leia (numero2)

    escreva ("A Soma é: ", numero1 + numero2, "\n")
    escreva ("A Subtração é: ", numero1 - numero2, "\n")
    escreva ("A Divisão é: ", numero1 / numero2, "\n")
    escreva ("A Multiplicação é: ", numero1 * numero2, "\n")



    
  }
}
